  "description": "turning-with-satoshi",
  "version": "1.0.0",
  "author": "",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "serve": "nodemon --watch API --watch DB --watch index.js --watch socket.js --watch lobby.js",
    "db": "mongod --dbpath=data",
    "dev": "npm-run-all --parallel watchify db",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "body-parser": "^1.18.2",
    "browser-process-hrtime": "^0.1.2",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "line-circle-collision": "^1.1.3",
    "mongodb": "^3.0.5",
    "node-gameloop": "^0.1.4",
    "passport": "^0.4.0",
    "passport-json": "^1.2.0",
    "random-js": "^1.0.8",
    "setimmediate": "^1.0.5",
    "socket.io": "^2.1.0",
    "triangle-circle-collision": "^1.0.4",
    "velocity-animate": "^2.0.5",
    "vue": "^2.0.1",
    "vue-checkbox-radio": "^0.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^1.0.6",
    "envify": "^3.4.1",
    "nodemon": "^1.17.2",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  }
}
